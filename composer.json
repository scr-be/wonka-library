{
    "name"        : "scribe/wonka-library",
    "license"     : "MIT",
    "type"        : "library",
    "description" : "Collection of framework-agnostic utilities and helper scripts.",
    "support" : {
        "email"   : "rmf@scr.be",
        "source"  : "https://scr.be/wonka-library/git",
        "issues"  : "https://scr.be/wonka-library/issues",
        "irc"     : "irc://irc.efnet.org/scribe"
    },
    "keywords"    : [
        "wonka-library", "scribe-wonka-library", "scr.be", "scribe", "scribenet", "scribe-softwade", "scribe-services"
    ],
    "authors" : [
        {
            "name"     : "Rob Frawley 2nd",
            "email"    : "rmf@scr.be",
            "homepage" : "https://scr.be/rmf",
            "role"     : "Project Lead"
        }
    ],
    "require" : {
        "php" : ">=5.6.4,~5.6"
    },
    "require-dev" : {
        "phpunit/phpunit" : "~4.6",
        "satooshi/php-coveralls" : "*",
        "scrutinizer/ocular" : "dev-master",
        "sami/sami" : "~3.0",
        "nikic/php-parser" : ">=1.0,<1.2"
    },
    "conflict" : {
        "scribe/mantle-bundle" : "<2.1.0"
    },
    "autoload" : {
        "psr-4" : {
            "Scribe\\Wonka\\" : "lib/"
        },
        "files" : [
            "stub/functions.php"
        ]
    },
    "config" : {
        "bin-dir"           : "bin",
        "minimum-stability" : "dev"
    },
    "extra" : {
        "branch-alias" : {
            "dev-master" : "0.1.x-dev"
        }
    }
}
