{
    "name"        : "scr-be/wonka-library",
    "license"     : "MIT",
    "type"        : "library",
    "homepage"    : "https://scr.be/wonka-library",
    "description" : "A collection of framework-agnostic functions and classes used as the base for many other Scribe projects.",
    "support" : {
        "email"  : "rmf@scr.be",
        "source" : "https://scr.be/wonka-library/git",
        "issues" : "https://scr.be/wonka-library/issues",
        "irc"    : "irc://irc.efnet.org/scribe"
    },
    "keywords"    : [
        "wonka-library", "scribe-wonka-library", "library", "scr.be", "scribe", "scribenet", "scribe-softwade", "scribe-services"
    ],
    "authors" : [
        {
            "name"     : "Rob Frawley 2nd",
            "email"    : "rmf@scr.be",
            "homepage" : "https://scr.be/rmf",
            "role"     : "Project Lead"
        }
    ],
    "require" : {
        "php" : ">=5.6.4,~5.6|~7.0"
    },
    "require-dev" : {
        "phpunit/phpunit"        : "~5.0",
        "codacy/coverage"        : "~1.0",
        "satooshi/php-coveralls" : "~0.8",
        "symfony/config"         : "~2.8"
    },
    "conflict" : {
        "scribe/mantle-bundle" : "<2.2.0"
    },
    "autoload" : {
        "psr-4" : {
            "Scribe\\Wonka\\" : "lib/"
        },
        "files" : [
            "lib/stub/functions-values.php",
            "lib/stub/functions-collections.php",
            "lib/stub/functions-plugins.php",
            "lib/stub/functions-deprecated.php"
        ]
    },
    "autoload-dev" : {
        "psr-4" : {
            "Scribe\\Wonka\\Tests\\" : "tests/"
        }
    },
    "minimum-stability" : "dev",
    "prefer-stable"     : true,
    "config": {
        "bin-dir" : "bin"
    },
    "extra" : {
        "branch-alias" : {
            "dev-master" : "0.3-dev"
        }
    }
}
